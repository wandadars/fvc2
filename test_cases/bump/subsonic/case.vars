! Subsonic bump-channel case (inlet Mach ~= 0.3).
! Non-dimensional values: rho=1.0, p=1.0, u=0.3*sqrt(gamma*p/rho)=0.355
time_step: 3e-4
output_interval: 5000
max_steps: 15000
gamma: 1.4
molar_mass: 8.3144598
diffusion_coeff: 1e-4
time_discretization: 1   ! 1=fwd euler, 2=rk4
convective_scheme: 1     ! 0=off, 1=ausm+, 2=roe

boundary_conditions:<
inlet=inlet(1.0 1.0 0.355 0.0)
outlet=subsonic_outlet(1.0)
bump=wall()
top_wall=wall()
symmetry=wall()
>

initial_condition_type: uniform
ic_uniform_state: 1.0 1.0 0.355 0.0

output_format_mode: vtk   ! tecplot | vtk | both, or 0/1/2
