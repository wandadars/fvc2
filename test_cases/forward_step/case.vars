! This case is a supersonic forward step case M=3.0.
! Conditions are:
! For air(Gamma=1.4): M=3.0, p=1 atm, T=300 K, u=1020 m/s
! Non-dimensional values: rho=1.0, p=1.0, u=3.0*sqrt(Gamma*p/rho)=1.952*3=5.856, v=0.0
time_step: 2e-5
output_interval: 2000
max_steps: 50000
gamma: 1.4
molar_mass: 8.3144598
diffusion_coeff: 1e-4
time_discretization: 1   ! 1=fwd euler, 2=rk4
convective_scheme: 1     ! 0=off, 1=ausm+, 2=roe

boundary_conditions:<
inlet=inlet(1.0 1.0 5.856 0.0)
outlet=extrapolated()
walls=wall()
>

initial_condition_type: uniform
ic_uniform_state: 1.0 1.0 5.856 0.0
shock_split_x: 0.5
shock_left_state: 1.0 1.0 5.856 0.0
shock_right_state: 1.0 1.0 5.856 0.0

output_format_mode: both   ! tecplot | vtk | both, or 0/1/2
